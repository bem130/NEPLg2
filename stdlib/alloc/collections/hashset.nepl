//: [注意/ちゅうい]:
//: - 引数の値は関数呼び出しで移動するため、再利用時は束縛し直してください。
//:
//: [計算量/けいさんりょう]:
//: - 本体処理に準じます。
fn hashset_remove <(HashSet,i32)*>Result<HashSet, Diag>> (hs, key):
    let hdr <i32> field::get hs "hdr"
    let cap <i32> load_i32 add hdr 4
    let entries <i32> load_i32 add hdr 8
    let entry_size <i32> 8
    let idx <i32> i32_rem_u hash32_i32 key cap
    let mut cur <i32> idx
    let mut done <i32> 0
    let mut removed <i32> 0
    while eq done 0:
        do:
            let entry <i32> add entries mul cur entry_size
            let occupied <i32> load_i32 entry
            if:
                eq occupied 0
                then set done 1
                else:
                    let ekey <i32> load_i32 add entry 4
                    if:
                        eq ekey key
                        then:
                            store_i32 entry 0
                            store_i32 hdr sub load_i32 hdr 1
                            set removed 1
                            set done 1
                        else:
                            set cur mod_s add cur 1 cap
                            if:
                                eq cur idx
                                then set done 1
                                else ()
    if:
        eq removed 1
        then:
            ok<HashSet, Diag> hs
        else:
            diag_err<HashSet> diag_key_not_found "hashset_remove"

