#import "std/mem"
#import "std/stdio"
#use std::mem::*
#use std::stdio::*
#entry "main"

fn main <()*>()> ():
    print "size_of<i32>: "
    let s_i32 <i32> size_of<i32>
    print_i32 s_i32
    print "\n"
    
    // pointer alloc
    let ptr <i32> alloc s_i32
    
    // generic store
    print "stored 456\n"
    store<i32> ptr 456
    
    // generic load
    print "load<i32>: "
    let val2 <i32> load<i32> ptr
    print_i32 val2
    print "\n"
    
    dealloc ptr s_i32
